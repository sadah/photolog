<div class="row photo-feed-wrapper">
  <div class="col-lg-12 photo-feed">
    <%= image_tag(@photo.image_url, class: "img-responsive") if @photo.image.present? %>
    <div class="photo-info">
      <h4><%= @photo.caption %></h4>
      <p>
        <%= link_to 'Edit', edit_photo_path(@photo) %>
        <%= link_to 'Destroy', @photo, method: :delete, data: { confirm: 'Are you sure?' } %>
      </p>
    </div>
  </div>
</div>
